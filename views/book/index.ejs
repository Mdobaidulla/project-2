<h1> All Books</h1>
<ul>
    <% books.forEach(book => { %>
    <table border="1">
       <tr>
           <td>
            <a href="/books/<%= book.id %>"><%= book.image %></a>
           </td>
           <td>
            <a href="/books/<%= book.id %>"><%= book.title %></a>
           </td>
       </tr>
    </table>

    <% }) %>
 </ul>