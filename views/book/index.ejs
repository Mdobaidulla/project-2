<h1> All Books</h1>
<a href="./books/new">Add New book</a>
<ul>
    <% books.forEach(book => { %>
    <table>
    
            <tr>
            <a href="/books/<%= book.id %>"> <img src="data:image/<%=book.image.contentType%>;base64, 
            <%=book.image.data.toString('base64')%>" style="height:300px;width:200px;"/></a>
           </tr><br>
            <tr>
            <a href="/books/<%= book.id %>"><%= book.title %></a>
           </tr>
       
    </table>

    <% }) %>


 </ul>


 